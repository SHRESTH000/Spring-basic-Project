<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <display-name>Customer</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>SaveCustomer.jsp</welcome-file>    
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>SaveCustomerServlet</display-name>
    <servlet-name>SaveCustomerServlet</servlet-name>
    <servlet-class>com.example.Customer.controller1.SaveCustomerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SaveCustomerServlet</servlet-name>
    <url-pattern>/SaveCustomerServlet</url-pattern>
  </servlet-mapping>
</web-app>